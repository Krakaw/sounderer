{
  "manifest_version": 2,
  "name": "Sounderer",
  "version": "1.1",
  "description": "Records audio from microphone and plays it back",
  "browser_action": {
    "default_title": "Sounderer"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
